<Workflow>
	<Start/>
	<Point name="p1"/>
	<Delay ms="100"/>

	<Condition name="cn_getInfoRatesData_checkRatest">
		<test expression="'(\w+)\s+(car)'" lang="regexp">
			<parameter value="$.ddddd" type="appData"/>
		</test>
		<iftrue>
			<GoTo actionName="p1"/>
			<UserTask name="UI3" />
		</iftrue>
	</Condition>
	<UserTask name="UI3" />
	
	<UserTask name="HardCodeFinishScreen"/>
	
	
	<Finish>
		<parameter name="routeServiceId" value="$.serviceId"  type="appData"/>
		<parameter name="routePointName" value="$.routePointName"  type="appData"/>
		<parameter name="destinationAction" value="endFlow" type="constant" />
	</Finish>
</Workflow>

 